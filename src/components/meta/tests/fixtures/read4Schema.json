{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "/",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "responseCode": {
            "id": "responseCode",
            "type": "integer",
            "enum": [
                404
            ]
        },
        "error": {
            "id": "error",
            "type": "string",
            "minLength": 1,
            "enum": [
                "Record not found"
            ]
        },
        "detail": {
            "id": "detail",
            "type": "string",
            "minLength": 1,
            "enum": [
                "Id(s): 0, 2, 4, 3"
            ]
        },
        "required": [
            "responseCode",
            "error",
            "detail"
        ]
    }
}